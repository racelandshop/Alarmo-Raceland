{"code":"import * as ca from './languages/ca.json';\r\nimport * as en from './languages/en.json';\r\nimport * as es from './languages/es.json';\r\nimport * as et from './languages/et.json';\r\nimport * as fr from './languages/fr.json';\r\nimport * as it from './languages/it.json';\r\nimport * as nl from './languages/nl.json';\r\nimport * as sv from './languages/sv.json';\r\nimport IntlMessageFormat from 'intl-messageformat';\r\nvar languages = {\r\n    ca: ca,\r\n    en: en,\r\n    et: et,\r\n    es: es,\r\n    fr: fr,\r\n    it: it,\r\n    nl: nl,\r\n    sv: sv,\r\n};\r\nexport function localize(string, language, ...args) {\r\n    const lang = language.replace(/['\"]+/g, '').replace('-', '_');\r\n    var translated;\r\n    try {\r\n        translated = string.split('.').reduce((o, i) => o[i], languages[lang]);\r\n    }\r\n    catch (e) {\r\n        translated = string.split('.').reduce((o, i) => o[i], languages['en']);\r\n    }\r\n    if (translated === undefined)\r\n        translated = string.split('.').reduce((o, i) => o[i], languages['en']);\r\n    if (!args.length)\r\n        return translated;\r\n    const argObject = {};\r\n    for (let i = 0; i < args.length; i += 2) {\r\n        let key = args[i];\r\n        key = key.replace(/^{([^}]+)?}$/, '$1');\r\n        argObject[key] = args[i + 1];\r\n    }\r\n    try {\r\n        const message = new IntlMessageFormat(translated, language);\r\n        return message.format(argObject);\r\n    }\r\n    catch (err) {\r\n        return 'Translation ' + err;\r\n    }\r\n}\r\n","references":["/workspaces/RacelandHA/config/custom_components/alarmo/frontend/localize/languages/ca.json","/workspaces/RacelandHA/config/custom_components/alarmo/frontend/localize/languages/en.json","/workspaces/RacelandHA/config/custom_components/alarmo/frontend/localize/languages/es.json","/workspaces/RacelandHA/config/custom_components/alarmo/frontend/localize/languages/et.json","/workspaces/RacelandHA/config/custom_components/alarmo/frontend/localize/languages/fr.json","/workspaces/RacelandHA/config/custom_components/alarmo/frontend/localize/languages/it.json","/workspaces/RacelandHA/config/custom_components/alarmo/frontend/localize/languages/nl.json","/workspaces/RacelandHA/config/custom_components/alarmo/frontend/localize/languages/sv.json","/workspaces/RacelandHA/config/custom_components/alarmo/frontend/node_modules/intl-messageformat/index.d.ts"]}
